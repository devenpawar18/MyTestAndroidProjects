<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:background="@layout/list_selector"
	android:layout_width="fill_parent" android:layout_height="?android:attr/listPreferredItemHeight">
	<RelativeLayout android:id="@+id/muttipleItemLayout"
		android:layout_width="fill_parent" 
		android:layout_margin="10dip"
		android:layout_height="wrap_content" android:layout_centerVertical="true">
		
		<TextView android:layout_height="wrap_content" android:id="@+id/dateTextView"
			android:textColor="@drawable/black"
			android:textSize="13dip" android:singleLine="true" android:ellipsize="end"
			android:layout_width="fill_parent">
		</TextView>
		<TextView android:layout_height="wrap_content" android:id="@+id/titleTextView"
			android:textSize="16dip" android:textStyle="bold" android:textColor="@drawable/black"
			android:layout_width="fill_parent" android:layout_below="@+id/dateTextView"
			android:singleLine="true"  android:ellipsize="end">
		</TextView>

		<TextView android:layout_height="wrap_content" android:id="@+id/locationTextView"
			android:textSize="13dip" android:singleLine="true" android:ellipsize="end"
			android:textColor="@drawable/black" android:textStyle="bold"
			android:layout_below="@+id/titleTextView" android:layout_width="fill_parent">
		</TextView>
		<TextView android:layout_height="wrap_content" android:id="@+id/localityTextView"
			android:textSize="13dip" android:singleLine="true" android:ellipsize="end"
			android:textColor="@drawable/black"
			android:layout_below="@+id/locationTextView" android:layout_width="fill_parent">
		</TextView>
	</RelativeLayout>
	<View android:id="@+id/divider" android:layout_width="fill_parent"
		android:layout_below="@+id/muttipleItemLayout" android:layout_height="2dip"
		android:background="@drawable/dividerColor"></View>
</RelativeLayout>
