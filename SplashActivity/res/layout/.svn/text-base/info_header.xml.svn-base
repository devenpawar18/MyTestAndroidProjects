<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="50dip" android:background="@drawable/white">
	<View android:layout_width="fill_parent" android:layout_height="1dip"
		android:background="@drawable/contactBorderColor" android:layout_alignParentBottom="true" />
	<TextView android:id="@+id/info_header_text" 
		android:textColor="@drawable/orangeColor" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_centerVertical="true"
		android:textStyle="bold" android:layout_marginLeft="10dip"
		android:gravity="center_vertical" android:text="About"
		android:textSize="16dip" android:singleLine="true" android:ellipsize="end" />
</RelativeLayout>
